package 十二次作业;


public class Dial extends AbstractDial{
	
	
	public Dial()
	{
		super();
	}
	
	public Dial(int lever)
	{
		super(lever);
	}
	
	public int getDial()
	{
		return dial;
	}
	
	public int dialUp()
	{
		if(dial < maxDial)
			dial++;
		return dial;
	}
	
	public int dialDown()
	{
		if(dial > 1)
			dial --;
		return dial;
	}
}

