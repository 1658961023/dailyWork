package 十二次作业;


public class DialNew extends AbstractDial implements Up_down{
	
	
	public DialNew()
	{
		super();
	}
	
	public DialNew(int lever)
	{
		super(lever);
	}
	
	public int getDial()
	{
		return dial;
	}
	@Override
	public void Up()
	{
		if(dial < maxDial)
			dial++;
	}
	
	public int dialDown()
	{
		if(dial > 1)
			dial --;
		return dial;
	}
}
