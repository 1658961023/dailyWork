package 十二次作业;


public class Lever extends AbstractLever{
	
	public Lever()
	{
		super();
	}
	
	public Lever(int maxlever)
	{
		super(maxlever);
	}
	
	public int getLever()
	{
		return lever;
	}
	
	public int leverUp()
	{
		if(lever < maxlever)
			lever++;
		return lever;
	}
	
	public int leverDown()
	{
		if(lever > 1)
			lever --;
		return lever;
	}
}
